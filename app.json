{
  "expo": {
    "name": "Mapin",
    "slug": "mapin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./shared/assets/img/icon.png",
    "scheme": "mapin",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./shared/assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mapin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./shared/assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.mapin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./shared/assets/img/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
